train_net: "examples/imagenet/train.prototxt"
test_net: "examples/imagenet/val.prototxt"
test_iter: 1000
test_interval: 1000
base_lr: 1e-05
display: 20
max_iter: 50000
lr_policy: "step"
gamma: 0.1
momentum: 0.9
weight_decay: 0.0005
stepsize: 100000
snapshot: 10000
snapshot_prefix: "examples/imagenet/snapshot/caffenet_train"
solver_mode: GPU
